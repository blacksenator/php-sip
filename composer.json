{
  "name": "level7systems/sip-php",
  "description": "PHP implementation of a simple SIP user agent",
  "type": "library",
  "authors": [
    {
      "name": "Chris Maciejewski"
    },
    {
      "name": "easybell Gmbh"
    }
  ],
  "autoload": {
    "psr-4": {
      "level7systems\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --coverage-html coverage"
  },
  "require": {
    "php": "^8.0",
    "ext-sockets": "*"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.30",
    "phpmd/phpmd": "^2.9",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
